first-release:
    rm -f first.exe
    as -g first.s -o first.o
    ld first.o -o first.exe
    rm first.o

first-debug:
    as -g first.s -o first.o
    ld first.o -o first.exe
    gdb first.exe
    rm first.o first.exe

second-release:
    rm -f second.exe
    as -g second.s -o second.o
    ld second.o -o second.exe
    rm second.o

second-debug:
    as -g second.s -o second.o
    ld second.o -o second.exe
    gdb second.exe
    rm second.o second.exe

third-release:
    rm -f third.exe
    as -g third.s -o third.o
    ld third.o -o third.exe
    rm third.o

third-debug:
    as -g third.s -o third.o
    ld third.o -o third.exe
    gdb third.exe
    rm third.o third.exe

third-explanation:
    cc -g -O0 third.c
    gdb ./a.out
    rm a.out
